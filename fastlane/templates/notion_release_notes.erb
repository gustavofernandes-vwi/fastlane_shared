# Release notes <%=version.title.title%>

<%
tasks.each do |task|
    line_url = task.public_url || task.url
    line_id = ""
    line_id = "[#{task.unique_id}](#{line_url}) " if task.unique_id != nil

    line_title = task.title.to_s
    line_title = "[#{task.title}](#{line_url})" if task.unique_id == nil
-%>
<%=line_id%><%=line_title%>

<% end %>